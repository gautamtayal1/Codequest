{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "globalEnv": ["JUDGE0_CALLBACK_URL", "NEXT_PUBLIC_JUDGE0_SERVER"],
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "check-types": {
      "dependsOn": ["^check-types"]
    },
    "dev": {
      "cache": false,
      "persistent": true,
      "env": ["JUDGE0_CALLBACK_URL", "NEXT_PUBLIC_JUDGE0_SERVER"]
    }
  }
}
